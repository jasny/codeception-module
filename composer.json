{
    "name": "jasny/codeception-module",
    "description": "Codeception Module for Jasny MVC",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "jasny/http-message": "^1.3",
        "jasny/router": "^1.1",
        "codeception/codeception": "^3.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "jasny/controller": "^1.0",
        "jasny/view": "^1.1.0",
        "twig/twig": "^1.29",
        "jasny/php-code-quality": "^2.1",
        "jasny/error-handler": "^0.2",
        "mouf/picotainer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Codeception\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/_support"]
    },
    "scripts": {
        "test": [
            "codecept run",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
